@model Geo.Itineraries.Web.Common.Models.MissingVenueModel

@{
    Layout = null;
}

<form id="deleteMissingVenue" method="post" role="form" action="@Url.Action("DeleteMissingVenue", "Venues")">
    <div class="modal-header">
        <h4 class="modal-title" id="myModalLabel">Are you sure you wish to delete this missing venue?</h4>
    </div>

    <div class="modal-body">

        @Html.AntiForgeryToken()

        <div class="form-group">
            @Html.LabelFor(model => model.VenueName, Model.VenueName, new { @class = "control-label" })
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" id="btnCancel">Cancel</button>
        <button type="submit" class="btn btn-danger" id="btnDeleteMissingVenue">Yes</button>
    </div>
</form>